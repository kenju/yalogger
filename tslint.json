{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": false,
    "max-classes-per-file": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "ordered-imports": [
      false
    ]
  }
}
